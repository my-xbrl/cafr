<?xml version="1.0" encoding="UTF-8"?>

<!-- US Comprehensive Annual Financial Report (CAFR) XBRL Taxonomy 2019-->
<!--Copyrights reserved for XBRL US -->
<!--Concepts and its details file  -->
<xsd:schema targetNamespace="https://xbrl.us/2019/us-cafr_type" attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:us-cafr_type="https://xbrl.us/2019/us-cafr_type" xmlns:link="http://www.xbrl.org/2003/linkbase" xmlns:xl="http://www.xbrl.org/2003/XLink" xmlns:xbrli="http://www.xbrl.org/2003/instance" xmlns:xlink="http://www.w3.org/1999/xlink">
  <xsd:import namespace="http://www.xbrl.org/2003/instance" schemaLocation="http://www.xbrl.org/2003/xbrl-instance-2003-12-31.xsd"/>
  <xsd:complexType name="TypeOfGovernment">
    <xsd:simpleContent>
      <xsd:restriction base="xbrli:stringItemType">
        <xsd:enumeration value="State"/>
        <xsd:enumeration value="County"/>
        <xsd:enumeration value="City"/>
        <xsd:enumeration value="Town"/>
        <xsd:enumeration value="Village"/>
        <xsd:enumeration value="Borough"/>
        <xsd:enumeration value="Other general purpose government"/>
        <xsd:enumeration value="School district"/>
        <xsd:enumeration value="Special district"/>
        <xsd:enumeration value="Township"/>
      </xsd:restriction>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="ReportingCurrency">
    <xsd:simpleContent>
      <xsd:restriction base="xbrli:stringItemType">
        <xsd:enumeration value="USD"/>
      </xsd:restriction>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="OrderOfMagnitude">
    <xsd:simpleContent>
      <xsd:restriction base="xbrli:stringItemType">
        <xsd:enumeration value="Dollars"/>
        <xsd:enumeration value="Thousands of Dollars"/>
        <xsd:enumeration value="Millions of Dollars"/>
      </xsd:restriction>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="ReportingFrequency">
    <xsd:simpleContent>
      <xsd:restriction base="xbrli:stringItemType">
        <xsd:enumeration value="Annual"/>
      </xsd:restriction>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="AuditStatus">
    <xsd:simpleContent>
      <xsd:restriction base="xbrli:stringItemType">
        <xsd:enumeration value="Audited"/>
        <xsd:enumeration value="Unaudited"/>
        <xsd:enumeration value="Reviewed"/>
        <xsd:enumeration value="Unreviewed"/>
      </xsd:restriction>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="AuditOpinion">
    <xsd:simpleContent>
      <xsd:restriction base="xbrli:stringItemType">
        <xsd:enumeration value="Qualified"/>
        <xsd:enumeration value="Adverse"/>
        <xsd:enumeration value="Unmodified"/>
        <xsd:enumeration value="Disclaimed"/>
      </xsd:restriction>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="TaxonomyVersion">
    <xsd:simpleContent>
      <xsd:restriction base="xbrli:stringItemType">
        <xsd:enumeration value="US CAFR XBRL Taxonomy v.0.3.0"/>
      </xsd:restriction>
    </xsd:simpleContent>
  </xsd:complexType>
</xsd:schema>
